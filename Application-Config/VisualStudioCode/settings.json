// Place your settings in this file to overwrite the default settings
{
  "cSpell.language": ",en,en-US",
  "C_Cpp.intelliSenseEngine": "Default",
  "editor.find.globalFindClipboard": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.insertSpaces": true,
  "editor.minimap.enabled": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    120
  ],
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "files.autoSave": "onFocusChange",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "window.openFoldersInNewWindow": "on",
  "window.restoreWindows": "all",
  "window.zoomLevel": 0,
  // Terminal Pro theme colors
  "workbench.colorCustomizations": {
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#990000",
    "terminal.ansiGreen": "#00a600",
    "terminal.ansiYellow": "#999900",
    "terminal.ansiBlue": "#0000b2",
    "terminal.ansiMagenta": "#b200b2",
    "terminal.ansiCyan": "#00a6b2",
    "terminal.ansiWhite": "#bfbfbf",
    "terminal.ansiBrightBlack": "#666666",
    "terminal.ansiBrightRed": "#e50000",
    "terminal.ansiBrightGreen": "#00d900",
    "terminal.ansiBrightYellow": "#e5e500",
    "terminal.ansiBrightBlue": "#0000ff",
    "terminal.ansiBrightMagenta": "#e500e5",
    "terminal.ansiBrightCyan": "#00e5e5",
    "terminal.ansiBrightWhite": "#e5e5e5",
  },
  "yarn.runInTerminal": true,
  "[json]": {
    "editor.formatOnSave": false,
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.formatOnSave": false,
    "editor.tabSize": 2
  },
  "bracketPairColorizer.forceIterationColorCycle": true,
  "bracketPairColorizer.forceUniqueOpeningColor": true,
  "bracketPairColorizer.showBracketsInGutter": true,
  "cSpell.enabledLanguageIds": [
    "c",
    "cpp",
    "csharp",
    "css",
    "go",
    "handlebars",
    "html",
    "jade",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pub",
    "python",
    "restructuredtext",
    "rust",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yml"
  ],
  "cSpell.userWords": [
    "Podfile"
  ],
  "debug.toolBarLocation": "docked",
  "editor.largeFileOptimizations": false,
  "git.allowForcePush": true,
  "git.rebaseWhenSync": true,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.historyExplorer.enabled": true,
  "gitlens.keymap": "chorded",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "search.showLineNumbers": true,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "workbench.editor.highlightModifiedTabs": true,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
